class Coord extends Object {
    protected float x;
    protected float y;
    protected float z;

    void setCoord(float x, float y, float z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }

    float getX() {
        return x;
    }

    float getY() {
        return y;
    }

    float getZ() {
        return z;
    }
}

{
    Coord c = new Coord();
    c.setCoord(8.56, 6, 54.526e-4F);
    if (c.getX() == 8.56 && c.getY() == 2*3.0 && c.getZ() == 54.526e-4f) {
        println("OK");
    }
}